{
  "name": "ziro.space",
  "version": "0.1.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "WATCHPACK_POLLING=true next start",
    "lint": "next lint",
    "test": "jest",
    "preview": "next build && next start",
    "test:watch": "jest --watch",
    "format": "prettier --write .",
    "analyze": "cross-env ANALYZE=true next build",
    "build:prod": "next build && next-bundle-analyzer",
    "docker:proxy": "docker-compose -f docker-compose.proxy.yml up",
    "migrate:latest": "npx knex migrate:latest --knexfile knexfile.js",
    "migrate:fresh": "npx knex migrate:fresh --knexfile knexfile.js",
    "docker:mac:run": "docker-compose -f docker-compose.mac.yml up",
    "docker:mac:build": "docker-compose -f docker-compose.mac.yml build",
    "docker:proxy:run": "docker compose -f docker-compose.proxy.yml up",
    "docker:proxy:build": "docker compose -f docker-compose.proxy.yml build",
    "db:seed": "NODE_OPTIONS='--import=tsx' TS_NODE_PROJECT=tsconfig-seed.json node --no-warnings prisma/seed.ts",
    "prisma:migrate": "npx prisma migrate dev",
    "prisma:reset": "npx prisma migrate reset"
  },
  "dependencies": {
