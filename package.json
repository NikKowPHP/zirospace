    "critters": "^0.0.23",
    "knex": "^3.1.0",
    "lucide-react": "^0.474.0",
    "next": "^14.2.24",
    "next-intl": "^3.26.3",
    "posthog-js": "^1.222.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.54.2",
