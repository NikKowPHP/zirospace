model ZirospaceScreenshotRating {
  id            BigInt              @id(map: "screenshot_ratings_pkey") @default(autoincrement())
  screenshot_id BigInt
  user_id       String?             @db.VarChar
  rating_value  Int
  created_at    DateTime            @default(now()) @db.Timestamptz(6)
  updated_at    DateTime            @default(now()) @updatedAt @db.Timestamptz(6)
  screenshot    ZirospaceScreenshot @relation(fields: [screenshot_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@map("zirospace_screenshot_ratings")
}

model ZirospaceScreenshot {
  id             BigInt                      @id(map: "screenshots_pkey") @default(autoincrement())
  app_id         BigInt
  image_url      String                      @db.VarChar
  screen_name    String?                     @db.VarChar
  route_path     String?                     @db.VarChar
  description    String?
  order_index    Int?                        @default(0)
  average_rating Float?                      @default(0)
  created_at     DateTime                    @default(now()) @db.Timestamptz(6)
  updated_at     DateTime                    @default(now()) @updatedAt @db.Timestamptz(6)
  ratings        ZirospaceScreenshotRating[]
  app            ZirospaceApp                @relation(fields: [app_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@map("zirospace_screenshots")
}

model ZirospaceAppTag {
  app_id     BigInt
  tag_id     BigInt
  created_at DateTime     @default(now()) @db.Timestamptz(6)
  app        ZirospaceApp @relation(fields: [app_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  tag        ZirospaceTag @relation(fields: [tag_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([app_id, tag_id])
  @@map("zirospace_app_tags")
}

model ZirospaceTag {
  id         BigInt            @id @default(autoincrement())
  name       String            @unique @db.VarChar
  created_at DateTime          @default(now()) @db.Timestamptz(6)
  updated_at DateTime          @default(now()) @updatedAt @db.Timestamptz(6)
  app_tags   ZirospaceAppTag[]

  @@map("zirospace_tags")
}

model ZirospaceAppRating {
  id           BigInt       @id(map: "app_ratings_pkey") @default(autoincrement())
  app_id       BigInt
  user_id      String?      @db.VarChar
  rating_value Int
  created_at   DateTime     @default(now()) @db.Timestamptz(6)
  updated_at   DateTime     @default(now()) @updatedAt @db.Timestamptz(6)
  app          ZirospaceApp @relation(fields: [app_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@map("zirospace_app_ratings")
}

model ZirospaceApp {
  id             BigInt                @id(map: "apps_pkey") @default(autoincrement())
  name           String                @db.VarChar
  description    String?
  thumbnail_url  String?               @db.VarChar
  average_rating Float?                @default(0)
  created_at     DateTime              @default(now()) @db.Timestamptz(6)
  updated_at     DateTime              @default(now()) @updatedAt @db.Timestamptz(6)
  ratings        ZirospaceAppRating[]
  app_tags       ZirospaceAppTag[]
  screenshots    ZirospaceScreenshot[]

  @@map("zirospace_apps")
}

