model zirospace_case_studies_en {
  id               String    @id
  slug             String    @unique
  title            String
  subtitle         String?
  description      String?
  tags             String[]
  case_study_images CaseStudyImageEN[]

  cta_text         String?
  cta_text_name    String?
  cta_url          String?
  color            String?   @db.VarChar(7)
  background_color String?   @db.VarChar(7)
  theme            String?   @db.VarChar(50)
  order_index      Int?      @default(0)
  created_at       DateTime? @default(now()) @db.Timestamptz(6)
  updated_at       DateTime? @default(now()) @db.Timestamptz(6)
}
model CaseStudyImageEN {
  id          String                    @id @default(cuid())
  url         String
  alt         String
  caseStudy   zirospace_case_studies_en @relation(fields: [caseStudyId], references: [id], onDelete: Cascade)
  caseStudyId String

  @@map("zirospace_case_study_images_en") // Maps to a good table name
}

model zirospace_case_studies_pl {
  id               String    @id
  slug             String    @unique
