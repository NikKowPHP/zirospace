/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model zirospace_app_ratings {
  id             BigInt         @id(map: "app_ratings_pkey") @default(autoincrement())
  app_id         BigInt
  user_id        String?        @db.VarChar
  rating_value   Int
  created_at     DateTime       @default(now()) @db.Timestamptz(6)
  updated_at     DateTime       @default(now()) @db.Timestamptz(6)
  zirospace_apps zirospace_apps @relation(fields: [app_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
}

/// This model contains row level security and requires additional setup for migrations. Visit https://pris.ly/d/row-level-security for more info.
model zirospace_app_tags {
  app_id         BigInt
  tag_id         BigInt
  created_at     DateTime       @default(now()) @db.Timestamptz(6)
  zirospace_apps zirospace_apps @relation(fields: [app_id], references: [id], onDelete: Cascade, onUpdate: NoAction)
  zirospace_tags zirospace_tags @relation(fields: [tag_id], references: [id], onDelete: Cascade, onUpdate: NoAction)

  @@id([app_id, tag_id])
}

model zirospace_apps {
